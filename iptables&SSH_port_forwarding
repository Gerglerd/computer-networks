sudo iptables -F
sudo iptables -A INPUT -i enp2s0 -p tcp --dport 22 -j ACCEPT
sudo iptables -A INPUT -i enp2s0 -m conntrack --ctstate ESTABLISHED,RELATED -j ACCEPT
sudo iptables -A INPUT -i enp2s0 -j DROP

ssh -L 8070:www.google.com:22 <hostname>
ssh -L 8080:ccfit.nsu.ru:22 <hostname>
ssh -L 8090:localhost:22 <hostname>

w - look your pts/number
to kill:
pkill -9 -t pts/number
in browser:
http://localhost/8090
