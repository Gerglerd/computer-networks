sudo iptables -F 
sudo iptables -A INPUT -i eth0 -m conntrack --ctstate ESTABLISHED,RELATED -j ACCEPT
sudo iptables -A INPUT -i eth0 -p TCP --dport 22 -j ACCEPT
sudo iptables -A INPUT -i eth0 -j DROP

